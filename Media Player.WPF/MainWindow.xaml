        xmlns:local="clr-namespace:Media_Player.WPF"
        mc:Ignorable="d"
        Title="Media Player" Height="450" Width="800" WindowStartupLocation="CenterScreen" ContentRendered="Window_ContentRendered" Closed="Window_Closed">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5"/>
